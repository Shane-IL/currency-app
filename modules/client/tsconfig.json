{
  "compilerOptions": {
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "noImplicitThis": true,
    "incremental": true,
    "target": "es2019",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "./bin",
    "composite": true
  },
  // "include": [
  //   "src"
  // ],
  // "references": [
  //   {
  //     "path": "./tsconfig.node.json"
  //   }
  // ]
}